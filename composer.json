{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2.3",
        "ext-redis": "*",
        "barryvdh/laravel-debugbar": "^3.8",
        "encore/laravel-admin": "1.*",
        "fideloper/proxy": "^4.4",
        "genealabs/laravel-model-caching": "^0.12.5",
        "guzzlehttp/guzzle": "^7.2",
        "huddledigital/zendesk-laravel": "^3.7",
        "jxlwqq/data-table": "^1.0",
        "laravel-admin-ext/chartjs": "^1.0",
        "laravel-admin-ext/grid-sortable": "^1.0",
        "laravel/framework": "^9.19",
        "laravel/horizon": "^5.15",
        "laravel/sanctum": "^3.0",
        "laravel/tinker": "^2.7",
        "rap2hpoutre/laravel-log-viewer": "^2.3",
        "spatie/eloquent-sortable": "^4.0",
        "spatie/laravel-activitylog": "^4.7",
        "spatie/laravel-collection-macros": "^7.12",
        "yajra/laravel-datatables-oracle": "^10.3"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "matthiasmullie/minify": "^1.3",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "remigrate": "@php artisan migrate:rollback && php artisan admin:install && php artisan db:seed",
        "assignments": "php artisan queue:work --queue unassignment-job,assignment-job,unassignment,assignment"
    }
}
